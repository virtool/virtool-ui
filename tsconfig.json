{
  "compilerOptions": {
    "baseUrl": "./src/js",
    "paths": {
      "@/*": ["./*"],
      "@account/*": ["./account/*"],
      "@administration/*": ["./administration/*"],
      "@app/*": ["./app/*"],
      "@base/*": ["./base/*"],
      "@files/*": ["./files/*"],
      "@forms/*": ["./forms/*"],
      "@groups/*": ["./groups/*"],
      "@hmms/*": ["./hmms/*"],
      "@indexes/*": ["./indexes/*"],
      "@jobs/*": ["./jobs/*"],
      "@labels/*": ["./labels/*"],
      "@message/*": ["./message/*"],
      "@ml/*": ["./ml/*"],
      "@nav/*": ["./nav/*"],
      "@otus/*": ["./otus/*"],
      "@quality/*": ["./quality/*"],
      "@references/*": ["./references/*"],
      "@samples/*": ["./samples/*"],
      "@sequences/*": ["./sequences/*"],
      "@subtraction/*": ["./subtraction/*"],
      "@types/*": ["./types/*"],
      "@users/*": ["./users/*"],
      "@utils/*": ["./utils/*"],
      "@wall/*": ["./wall/*"],
    },
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "checkJs": false,
    "esModuleInterop": false,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["DOM", "ESNext", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "node",
    "noEmit": true,
    "outDir": "./build/",
    "rootDir": "./src",
    "skipLibCheck": true,
    "strict": false,
    "target": "ESNext",
    "typeRoots": ["node_modules/@types", "./src/js/types"]
  },
  "include": ["src/**/*"]
}
