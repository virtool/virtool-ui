{
    "compilerOptions": {
        "baseUrl": "./src",
        "paths": {
            "@/*": ["./js/*"],
            "@account/*": ["./js/account/*"],
            "@administration/*": ["./js/administration/*"],
            "@app/*": ["./js/app/*"],
            "@base": ["./js/base/index.js"],
            "@base/*": ["./js/base/*"],
            "@files/*": ["./js/files/*"],
            "@forms/*": ["./js/forms/*"],
            "@groups/*": ["./js/groups/*"],
            "@hmms/*": ["./js/hmms/*"],
            "@indexes/*": ["./js/indexes/*"],
            "@jobs/*": ["./js/jobs/*"],
            "@labels/*": ["./js/labels/*"],
            "@message/*": ["./js/message/*"],
            "@ml/*": ["./js/ml/*"],
            "@nav/*": ["./js/nav/*"],
            "@otus/*": ["./js/otus/*"],
            "@quality/*": ["./js/quality/*"],
            "@references/*": ["./js/references/*"],
            "@samples/*": ["./js/samples/*"],
            "@sequences/*": ["./js/sequences/*"],
            "@subtraction/*": ["./js/subtraction/*"],
            "@tests/*": ["./tests/*"],
            "@users/*": ["./js/users/*"],
            "@utils/*": ["./js/utils/*"],
            "@wall/*": ["./js/wall/*"]
        },
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "checkJs": false,
        "esModuleInterop": false,
        "isolatedModules": true,
        "jsx": "react",
        "lib": ["DOM", "ESNext", "DOM.Iterable"],
        "module": "ESNext",
        "moduleResolution": "node",
        "noEmit": true,
        "outDir": "./build/",
        "rootDir": "./src",
        "skipLibCheck": true,
        "strict": false,
        "target": "ESNext",
        "typeRoots": ["node_modules/@types", "./src/js/types"]
    },
    "include": ["src/**/*"]
}
