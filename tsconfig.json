{
    "compilerOptions": {
        "baseUrl": "./src",
        "paths": {
            "@/*": ["./*"],
            "@account/*": ["./account/*"],
            "@administration/*": ["./administration/*"],
            "@analyses/*": ["./analyses/*"],
            "@app/*": ["./app/*"],
            "@base/*": ["./base/*"],
            "@dev/*": ["./dev/*"],
            "@forms/*": ["./forms/*"],
            "@groups/*": ["./groups/*"],
            "@hmm/*": ["./hmm/*"],
            "@indexes/*": ["./indexes/*"],
            "@jobs/*": ["./jobs/*"],
            "@labels/*": ["./labels/*"],
            "@message/*": ["./message/*"],
            "@ml/*": ["./ml/*"],
            "@nav/*": ["./nav/*"],
            "@otus/*": ["./otus/*"],
            "@quality/*": ["./quality/*"],
            "@references/*": ["./references/*"],
            "@samples/*": ["./samples/*"],
            "@sequences/*": ["./sequences/*"],
            "@subtraction/*": ["./subtraction/*"],
            "@tests/*": ["./tests/*"],
            "@uploads/*": ["./uploads/*"],
            "@users/*": ["./users/*"],
            "@wall/*": ["./wall/*"]
        },
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": false,
        "isolatedModules": true,
        "jsx": "react",
        "lib": ["DOM", "ESNext", "DOM.Iterable"],
        "module": "ESNext",
        "moduleResolution": "node",
        "noEmit": true,
        "outDir": "./build/",
        "rootDir": "./src",
        "skipLibCheck": true,
        "strict": false,
        "target": "ESNext",
        "typeRoots": ["node_modules/@types", "./src/types"]
    },
    "include": ["src/**/*"]
}
